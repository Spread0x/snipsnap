{
  "name": "snipsnap-vscode-templates",
  "displayName": "Snipsnap Templates",
  "publisher": "pixel-point",
  "description": "Generate custom templates on a whim",
  "version": "0.3.4",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "react",
    "generate",
    "component",
    "boilerplate"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "snipsnap-templates.runExtension",
        "title": "Create from Template",
        "category": "Snipsnap"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "snipsnap-templates.runExtension",
          "when": "false"
        }
      ],
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "snipsnap-templates.runExtension",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  },
  "dependencies": {
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.lowercase": "^4.3.0",
    "lodash.snakecase": "^4.1.1",
    "lodash.startcase": "^4.4.0",
    "lodash.uppercase": "^4.3.0",
    "mustache": "^4.1.0"
  }
}
